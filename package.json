    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "ts-node": "^8.10.2",
    "tslib": "^2.3.0",
    "typescript": "^3.9.9"
  },
